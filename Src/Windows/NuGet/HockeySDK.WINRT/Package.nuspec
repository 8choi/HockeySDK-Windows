<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>HockeySDK.WINRT</id>
    <version>2.2.2</version>
    <authors>Bit Stadium GmbH</authors>
  <owners>Bit Stadium GmbH</owners>
    <licenseUrl>https://github.com/bitstadium/HockeySDK-Windows/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/bitstadium/HockeySDK-Windows</projectUrl>
    <iconUrl>https://rink.hockeyapp.net/images/icon@2x.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Use the HockeyApp platform from Windows apps. Collect crash reports, provide feedback-functionality and automatic updates.</description>
    <language>en-US</language>
    <copyright>Copyright (c) 2015 Bit Stadium GmbH</copyright>
    <tags>crash reports reporting feedback windows hockeyapp</tags>

    <dependencies>
       <dependency id="HockeySDK.Core" version="[2.2.2]" />
    </dependencies>
  </metadata>

  <files>
    <!-- HockeySDK -->
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_Win81\HockeyApp.dll" target="lib\portable-win81" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_Win81\HockeyApp.pri" target="lib\portable-win81" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_Win81\HockeyApp.pdb" target="lib\portable-win81" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_Win81\HockeyApp.xr.xml" target="lib\portable-win81\HockeyApp" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_Win81\Views\*.xbf" target="lib\portable-win81\HockeyApp\Views" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_Win81\Assets\*.*" target="lib\portable-win81\HockeyApp\Assets" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_WP81\HockeyApp.dll" target="lib\portable-wpa81" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_WP81\HockeyApp.pri" target="lib\portable-wpa81" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_WP81\HockeyApp.pdb" target="lib\portable-wpa81" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_WP81\HockeyApp.xr.xml" target="lib\portable-wpa81\HockeyApp" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_WP81\Views\*.xbf" target="lib\portable-wpa81\HockeyApp\Views" />
    <file src="$configuration$\Windows\HockeyApp\HockeySDK_WP81\Assets\*.*" target="lib\portable-wpa81\HockeyApp\Assets" />
    <file src="$configuration$\src\Windows\NuGet\HockeySDK.WP\tools\**" target="tools" />
  
    <!-- Application Insights -->
    <file src="$configuration$\Windows\Wrt81\Microsoft.ApplicationInsights.*" target="lib\portable-win81" />
  </files>
</package>